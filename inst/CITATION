  title        = "Causal Discovery with Cascade Nonlinear Additive Noise Models",
  author       = personList(as.person("Ruichu Cai"),
                   as.person("Jie Qiao"),
                   as.person("Kun Zhang"),
                   as.person("Zhenjie Zhang"),
                   as.person("Zhifeng Hao")),
