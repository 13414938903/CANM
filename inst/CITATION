  year         = "2019",
  textVersion  = paste("Ruichu Cai, Jie Qiao, Kun Zhang, Zhenjie Zhang, Zhifeng Hao.",
  "Causal Discovery with Cascade Nonlinear Additive Noise Models.",
  "IJCAI 2019.")
)
